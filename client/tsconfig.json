{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",


    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": true,
    "types": ["nuxt", "vue", "node", "@types/node"]
  },
  "exclude": ["node_modules", "dist"],
  "include": [
    "nuxt.config.ts",
    "app.config.ts",
    ".nuxt/nuxt.d.ts",
    "**/*.ts",
    "**/*.d.ts",
    "**/*.vue",
    ".nuxt",
    "server"
  ]
}
